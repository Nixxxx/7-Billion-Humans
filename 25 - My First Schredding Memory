mem2 = nearest shredder
a:
mem1 = nearest datacube
pickup mem1
giveto mem2
jump a


-- 7 Billion Humans (2059) --
-- 25: My First Shredding Memory --
-- Nixxx : 10/5 131/132


mem3 = nearest datacube
mem2 = nearest shredder
a:
mem1 = nearest datacube
if mem1 != nothing:
	pickup mem1
	if mem1 != nothing:
		giveto mem2
	endif
else:
	end
endif
jump a


